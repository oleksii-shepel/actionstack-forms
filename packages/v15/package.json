{
  "name": "nygma",
  "version": "15.0.0",
  "description": "Forms Add-On for NgRx store",
  "license": "MIT",
  "keywords": [
    "ngrx",
    "angular",
    "forms",
    "reactive",
    "template-driven",
    "sync",
    "nygma",
    "store"
  ],
  "module": "public-api.ts",
  "peerDependencies": {
    "@angular/core": "~15.0.0",
    "@angular/forms": "~15.0.0",
    "@ngrx/store": "~15.0.0",
    "rxjs": "~7.8.0"
  },
  "devDependencies": {
    "tslib": "~2.5.2"
  },
  "scripts": {
    "ng": "ng",
    "start": "cd .. && ng serve",
    "build": "cd .. && ng build",
    "watch": "cd .. && ng build --watch --configuration development",
    "test": "cd .. && jest --verbose",
    "test:coverage": "cd .. && jest --coverage --watchAll=false",
    "test:watch": "cd .. && jest --watch",
    "deploy": "cd .. && ng deploy --base-href=/angular-ngrx-forms/",
    "eslint": "cd .. && eslint ./projects/nygma/src/lib --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oleksii-shepel/angular-ngrx-reactive-forms.git"
  },
  "author": "Oleksii Shepel",
  "bugs": {
    "url": "https://github.com/oleksii-shepel/angular-ngrx-reactive-forms/issues"
  },
  "homepage": "https://github.com/oleksii-shepel/angular-ngrx-reactive-forms#readme"
}
